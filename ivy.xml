		<dependency org="org.hibernate" name="hibernate-entitymanager" rev="4.1.7.Final" conf="build->default" />
		<!--<dependency org="org.slf4j" name="slf4j-log4j12" rev="1.6.1" conf="build->default"/>-->
		<!--<dependency org="org.hibernate" name="hibernate-entitymanager" rev="3.6.10.Final" conf="build->default" />-->
	</dependencies>
</ivy-module>
